{
  "name": "slack-social-api",
  "version": "0.0.0",
  "description": "REST Server for APT",
  "keywords": [
    "node",
    "express",
    "mongodb"
  ],
  "author": "",
  "homepage": "",
  "license": "",
  "scripts": {
    "rimraf": "rimraf",
    "clean": "npm cache clean && npm run rimraf -- node_modules doc coverage",
    "preclean:install": "npm run clean",
    "clean:install": "npm set progress=false && npm install",
    "preclean:start": "npm run clean",
    "clean:start": "npm start",
    "watch": "npm run watch:dev",
    "watch:dev": "npm run build:dev -- --watch",
    "watch:dev:hmr": "npm run watch:dev -- --hot",
    "watch:test": "npm run test -- --auto-watch --no-single-run",
    "watch:prod": "npm run build:prod -- --watch",
    "server": "npm run server:dev",
    "server:dev": "nodemon ./server.js",
    "server:stage": "NODE_ENV=staging node server.js",
    "server:prod": "NODE_ENV=production node server.js",
    "fix:line-endings": "find . \\( -type f -and -not -path '*/node_modules/*' \\) -exec dos2unix {} \\;",
    "test": "npm run lint && NODE_ENV=test mocha",
    "start": "node server.js",
    "preversion": "npm test",
    "version": "npm run build",
    "postversion": "git push && git push --tags",
    "lint": "eslint 'app/**/*.js' 'test/**/*.js'",
    "lint:html": "eslint 'app/**/*.js' 'test/**/*.js' -f html -o ./reports/lint-results.html",
    "lint:fix": "eslint --fix 'app/**/*.js' 'test/**/*.js'"
  },
  "dependencies": {
    "agenda": "^1.0.3",
    "async": "^2.4.1",
    "avsc": "^5.0.4",
    "axios": "^0.17.1",
    "bluebird": "^3.5.0",
    "body-parser": "^1.17.2",
    "cookie-parser": "^1.4.3",
    "express": "^4.15.2",
    "http": "0.0.0",
    "method-override": "^2.3.6",
    "mongoose": "^4.4.19",
    "path": "^0.12.7",
    "promise": "^7.1.1",
    "request": "^2.81.0",
    "rimraf": "^2.5.2",
    "socket.io": "^1.4.6",
    "uuid": "^3.0.1",
    "winston": "^2.3.1"
  },
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-core": "^6.9.1",
    "babel-loader": "^6.2.4",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.9.0",
    "babel-runtime": "^6.9.2",
    "chai": "^3.5.0",
    "del": "^2.2.0",
    "eslint": "^4.1.1",
    "gulp": "^3.9.1",
    "gulp-docco": "0.0.4",
    "gulp-nodemon": "^2.0.7",
    "gulp-rename": "^1.2.2",
    "gulp-util": "^3.0.7",
    "http-server": "^0.9.0",
    "mocha": "^2.5.3",
    "morgan": "^1.7.0",
    "nodemon": "^1.11.0",
    "sinon": "2.3.5"
  },
  "engines": {
    "node": ">= 4.2.1",
    "npm": ">= 3"
  }
}
